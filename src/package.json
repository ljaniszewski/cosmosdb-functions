  "version": "1.0",
  "description": "Sample CosmosDB triggers and stored procedures",
  "scripts": {
    "start": "babel src --out-dir dist"
  },
  "babel": {
    "presets": [
      [
        "minify",
        {
          "removeConsole": true
        }
      ]
