    "@babel/core": "^7.1.2",
    "babel-preset-minify": "^0.5.0",
    "babel-plugin-loop-optimizer": "^1.4.1",
    "crypto": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:ljaniszewski/cosmosdb-functions.git"
  }
}